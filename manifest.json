{
  "version": "0.1",
  "name": "Corona Detector",
  "description": "Find corona on the site and replace by an icon",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon/icon@16.png",
    "32": "assets/icon/icon@32.png",
    "48": "assets/icon/icon@48.png",
    "128": "assets/icon/icon@128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}